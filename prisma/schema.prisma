generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:root@localhost:5432/merchant_tests?schema=public"
}

model users {
  id        Int      @id @default(autoincrement())
  username  String
  email     String   @unique
  password  String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
